<script>
  function watsappsessionapicall(phone_no) {
    const update = {
      phone: `${phone_no}`,
      media: {
        type: "interactive_list",
        header: {
          text: "Route Mobile"
        },
        body: "Hi! It's the Route Mobile Support Assistant. What can I help you with?",
        footer_text: "Menu",
        button_text: "Select",
        button: [
          {
            section_title: "Chicken",
            row: [
              {
                id: "1",
                title: "Issue with item delivery",
              }
            ]
          },
          {
            section_title: "Fish",
            row: [
              {
                id: "2",
                title: "Invoice/Bill",
              }
            ]
          },
          {
            section_title: "Fish",
            row: [
              {
                id: "3",
                title: "Others",
              }
            ]
          }
        ]
      }
    };
    const options = {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
        'Authorization': '6188c2110fcc5f00129106d7',
      },
      body: JSON.stringify(update),
    };
    fetch('https://rapidapi.rmlconnect.net/wbm/v1/message', options)
      .then(data => {
        if (!data.ok) {
          throw Error(data.status);
        }
        return data.json();
      }).then(update => {
        console.log(update);
      }).catch(e => {
        console.log(e);
      });
  }

  function watsappsessioncallfunc() {
    let phone = prompt('Type your number here');
    watsappsessionapicall(phone);
  }

  const customercare = document.querySelector(".watsappmsg");
  customercare.addEventListener("click", watsappsessioncallfunc);
</script>

